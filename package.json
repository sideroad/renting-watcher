  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/node": "^20.14.14",
    "@typescript-eslint/eslint-plugin": "^7.17.0",
    "@typescript-eslint/parser": "^7.17.0",
    "eslint": "^8.57.0",
    "jest": "^30.0.5",
    "ts-jest": "^29.4.0",
    "tsx": "^4.16.5",
    "typescript": "^5.5.4"
